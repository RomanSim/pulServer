create table Company(email varchar(255) not null ,CompID int primary key auto_increment not null,CompName varchar(50) not null,username varchar(50) not null,password varchar(50) not null);

create table Computers(CompID int not null , Comp int  primary key not null, foreign key(CompID) references Company(CompID));

create table Statistics(Comp int not null,TimeOfStat datetime not null,TypeOfStat varchar(50) not null,Content float not null, foreign key(Comp) references Computers(Comp));

create table Logs(textCont varchar(255) , Comp int not null,LogType varchar(50) not null,Time datetime not null, Process tinyint(1), foreign key(Comp) references Computers(Comp));

create table Commands(TimeOfCreation datetime not null , Comp int not null,TimeOfExecution datetime not null,Command varchar(100) , Outcome varchar(100),ErrorsT tinyint(1), foreign key(Comp) references Computers(Comp));

create table Alerts(TimeRes datetime not null , Comp int not null,SeverityLevel varchar(50) not null,AlertDescription varchar(100) not null, ShowAlertUn tinyint(1),ReadByUser tinyint(1), foreign key(Comp) references Computers(Comp));